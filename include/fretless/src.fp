zoom 0.8
rotn -90

# Fretless guitar
frets 0 12

# Uncomment the following line to remove the nut and the bridge
# void frets

# Add some notes
notes <6,5> <3,6>

nx<6,5> shape=circle,draw=red,text=white,fill=red,inner sep=1.5
nl<6,5> {\scriptsize A}

nx<3,6> shape=circle,draw=red,text=blue,fill=white,inner sep=0.3
nl<3,6> {\scriptsize C$\sharp$}
